@model StoreUser
@{
    ViewData["Title"] = "商家信息";
}

<h1 class="text-center">详细信息</h1>


<div class="row justify-content-center">
    <div class="col-md-8 col-sm-10 col-lg-4">
        <hr />

        <dl class="row offset-2">
            <dt class="col-6">@Html.DisplayNameFor(model => model.StoreName)</dt>
            <dd class="col-6">@Html.DisplayFor(model => model.StoreName)</dd>

            <dt class="col-6">@Html.DisplayNameFor(model => model.Account)</dt>
            <dd class="col-6">@Html.DisplayFor(model => model.Account)</dd>

            <dt class="col-6">@Html.DisplayNameFor(model => model.PassWord)</dt>
            <dd class="col-6">@Html.DisplayFor(model => model.PassWord)</dd>

            <dt class="col-6">@Html.DisplayNameFor(model => model.RegisterTime)</dt>
            <dd class="col-6">@Html.DisplayFor(model => model.RegisterTime)</dd>

            <dt class="col-6">@Html.DisplayNameFor(model => model.StoreType.TypeName)</dt>
            <dd class="col-6">@Html.DisplayFor(model => model.StoreType.TypeName)</dd>

        </dl>

        <hr />

        <div class="container offset-2">
            <a class="btn btn-outline-danger" asp-action="StoreUserEdit" asp-route-id="@Model.ID">编辑</a>
            <a class="btn btn-outline-primary" asp-action="StoreUsersIndex">返回</a>
        </div>
        
    </div>

</div>